{
  "name": "home0001",
  "private": true,
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "npm-run-all --sequential clean generate:* --parallel build:next build:sanity",
    "dev": "npm-run-all --sequential generate:* --parallel dev:*",
    "start": "npm-run-all --sequential generate:* start:next",
    "clean": "rimraf ./public/studio",
    "generate:sanity": "yarn run sanity-codegen",
    "build:next": "next build --debug",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "dev:next": "next dev",
    "dev:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "start:next": "next start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.11",
