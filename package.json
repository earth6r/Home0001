    "next-sanity-image": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scroll": "^1.8.9",
    "react-toastify": "^9.1.1",
    "schema-dts": "^1.1.0",
    "slugify": "^1.6.5",
    "swiper": "^9.0.5"
  },
