    "@sanity/client": "^3.2.0",
    "@sanity/image-url": "^1.0.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "focus-visible": "^5.2.0",
    "groq": "^2.15.0",
    "lodash": "^4.17.21",
